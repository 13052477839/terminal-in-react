{
  "name": "terminal-in-react",
  "version": "1.0.3",
  "description": "A component for making a terminal in React",
  "main": "index.js",
  "repository": "https://www.github.com/nitin42/terminal-in-react",
  "author": "Nitin Tulswani",
  "license": "MIT",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "files": [
    "build",
    "index.js"
  ],
  "devDependencies": {
    "babel-loader": "^7.1.1",
    "babel-preset-react-app": "^3.0.1",
    "babili-webpack-plugin": "^0.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "html-webpack-plugin": "^2.29.0",
    "html-webpack-template": "^6.0.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  },
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config ./webpack.config.js --open --progress --colors --content-base starter/ --hot",
    "build:production": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack/webpack.config.js --display-max-modules 0",
    "prepublish": "npm run build:production",
    "prebuild": "rm -rf ./build"
  },
  "keywords": [
    "react",
    "terminal",
    "simple",
    "display"
  ]
}
